  "tests": "Test.assertEquals(letterCounter([\n\t['D', 'E', 'Y', 'H', 'A', 'D'],\n\t['C', 'B', 'Z', 'Y', 'J', 'K'],\n\t['D', 'B', 'C', 'A', 'M', 'N'],\n\t['F', 'G', 'G', 'R', 'S', 'R'],\n\t['V', 'X', 'H', 'A', 'S', 'S']\n], 'D'), 3)\n\nTest.assertEquals(letterCounter([\n\t['D', 'E', 'Y', 'H', 'A', 'D'],\n\t['C', 'B', 'Z', 'Y', 'J', 'K'],\n\t['D', 'B', 'C', 'A', 'M', 'N'],\n\t['F', 'G', 'G', 'R', 'S', 'R'],\n\t['V', 'X', 'H', 'A', 'S', 'S']\n], 'H'), 2)\n\nTest.assertEquals(letterCounter([\n\t['D', 'E', 'Y', 'H', 'A', 'D'],\n\t['C', 'B', 'Z', 'Y', 'J', 'K'],\n\t['D', 'B', 'C', 'A', 'M', 'N'],\n\t['F', 'G', 'G', 'R', 'S', 'R'],\n\t['V', 'X', 'H', 'A', 'S', 'S']\n], 'Z'), 1)\n\nTest.assertEquals(letterCounter([\n\t['D', 'E', 'Y', 'H', 'A', 'D'],\n\t['C', 'B', 'Z', 'Y', 'J', 'K'],\n\t['D', 'B', 'C', 'A', 'M', 'N'],\n\t['F', 'G', 'G', 'R', 'S', 'R'],\n\t['V', 'X', 'H', 'A', 'S', 'S']\n], 'R'), 2)\n\nTest.assertEquals(letterCounter([\n\t['D', 'E', 'Y', 'H', 'A', 'D'],\n\t['C', 'B', 'Z', 'Y', 'J', 'K'],\n\t['D', 'B', 'C', 'A', 'M', 'N'],\n\t['F', 'G', 'G', 'R', 'S', 'R'],\n\t['V', 'X', 'H', 'A', 'S', 'S']\n], 'X'), 1)\n\nTest.assertEquals(letterCounter([\n\t['D', 'E', 'Y', 'H', 'A', 'D'],\n\t['C', 'B', 'Z', 'Y', 'J', 'K'],\n\t['D', 'B', 'C', 'A', 'M', 'N'],\n\t['F', 'G', 'G', 'R', 'S', 'R'],\n\t['V', 'X', 'H', 'A', 'S', 'S']\n], 'S'), 3)",
